<?xml version="1.0"?>
<!--FIX Simple Binary Encoding message template -->
<sbe:messageSchema xmlns:sbe="http://www.fixprotocol.org/ns/simple/1.0"
		   xmlns:xi="http://www.w3.org/2001/XInclude"
		   package="sample" version="FIX.5.0" description="MDOE" byteOrder="bigEndian">

  <!--  xi:include href="sbe-builtins.xml"/ -->

  <types>
    <!-- Simple field types -->
    <type name="Boolean" primitiveType="Boolean" fixUsage="Boolean" />
    <type name="Int64" primitiveType="Int64" fixUsage="int" />
    <enum name="Status" encodingType="charEnum">
      <validValue name="Reject" description="Order Rejection">8</validValue>
      <validValue name="Create" description="Order Creation">0</validValue>
      <validValue name="Cancel" description="">4</validValue>
      <validValue name="Modify" description="">5</validValue>
      <validValue name="PF" description="Partial Fill">1</validValue>
      <validValue name="Fill" description="">2</validValue>
      <validValue name="TradeCancel" description="">H</validValue>
      <validValue name="TradeModify" description="">G</validValue>
    </enum>	
    <composite name="decimal32" fixUsage="Price">
      <type name="mantissa" primitiveType="int32" />
      <type name="exponent" primitiveType="int8" presence="constant">-4</type>
    </composite>
    <enum name="Side" encodingType="charEnum">
      <validValue name="Buy" description="Buy">1</validValue>
      <validValue name="Sell" description="Sell">2</validValue>
    </enum>		
    <type name="Int32" primitiveType="Int32" fixUsage="int" />
    <type name="timestamp" primitiveType="Int64" timeUnit="nanosecond" fixUsage="UTCTimestamp" />
  </types>

  <sbe:message name="MDOE" id="1"
	       description="Order Entry Data for Book Reconstruction and MD generation">
    <field type="Boolean" name="EoE" 
	   description="End of Event Indicator" />
    <field type="Status" name="Status"/>
    <field type="Int64" name="BookQty"
	   description="Book Quantity" />
    <field type="Int64" name="SecurityID"/>
    <field type="Int64" name="OrderID"/>
    <field type="decimal32" name="BookPrice" />	
    <field type="Side" name="Side" />
    <field type="decimal32" name="FillPrice" />	
    <field type="Int64" name="FillQty" 
	   description="Fill Quantity" />
    <field type="Int64" name="TradeID"/>
    <field type="Int32" name="NumRealOrd" offset="0"
	   description="Number of Real Orders(Cons Fill)" />
    <field type="Boolean" name="Aggressor" offset="0"/>
    <field type="timestamp" name="EngineStart" />
    <field type="timestamp" name="EoETime"
	   description="The nanosecond end time of the event" />
  </sbe:message>
</sbe:messageSchema>