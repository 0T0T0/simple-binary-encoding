<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<sbe:messageSchema xmlns:sbe="http://www.fixprotocol.org/ns/simple/1.0"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   package="SBE tests"
                   fixVersion="5.2"
                   description="Unit Test"
                   byteOrder="littleEndian"
                   xsi:schemaLocation="http://www.fixprotocol.org/ns/simple/1.0/SimpleBinary1-0.xsd">
    <types>
        <type name="Symbol" description="Symbol" length="20" primitiveType="char" fixUsage="char"/>
        <composite name="messageHeader" description="Template ID and length of message root" fixUsage="Length">
            <type name="blockLength" primitiveType="uint16"/>
            <type name="templateId" primitiveType="uint16"/>
            <type name="version" primitiveType="uint8"/>
            <type name="reserved" primitiveType="uint8"/>
        </composite>
    </types>
    <sbe:message name="TestMessage1" id="1" description="TestMessage" blockLength="16">
        <field name="Tag1" id="1" type="uint32" fixUsage="int"/>
        <field name="NoEntries" id="2" type="uint8" groupName="Entries" fixUsage="NumInGroup"/>
        <group name="Entries">
            <field name="TagGroup1" id="3" type="Symbol"/>
            <field name="TagGroup2" id="4" type="int64" fixUsage="int"/>
        </group>
    </sbe:message>
</sbe:messageSchema>
